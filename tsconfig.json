{
  "compilerOptions": {
    "allowJs": false,
    "noImplicitAny": true,
    "noEmitOnError": true,
    "declaration": true,
    "removeComments": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "strictNullChecks": false,
    "skipLibCheck": true,
    "importHelpers": true,
    "target": "es2016",
    "lib": ["dom", "es2016"],
    "module": "commonjs",
    "rootDir": "src",
    "outDir": "dist",
    "moduleResolution": "node",
    "jsx": "react",
    "types": ["node"]
  },
  "include": [
  ],
  "files": [
    "src/hear-ye.ts",
    "src/ui/hear-ye-ui-api.ts"
  ]
}
